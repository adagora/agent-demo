{
  "name": "gora",
  "version": "0.1.0",
  "description": "Gora - AI Code Editing Agent with multi-agent orchestration and feedback loops",
  "type": "module",
  "main": "dist/gora.js",
  "bin": {
    "gora": "dist/gora.js"
  },
  "scripts": {
    "build": "bun build src/gora.ts --outdir dist --target bun",
    "start": "bun dist/gora.js",
    "dev": "bun --watch src/gora.ts",
    "gora": "bun src/gora.ts",
    "typecheck": "bun x tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "bun x eslint src --ext .ts",
    "lint:fix": "bun x eslint src --ext .ts --fix",
    "format": "bun x prettier --write src/**/*.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.21.0",
    "openai": "^4.77.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "prettier": "^3.4.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0"
  }
}
